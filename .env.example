# Firebase Configuration Example
# Copy this file to .env and fill in your actual values

# Firebase Project Settings
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-storage-bucket.appspot.com

# Firebase Android Configuration
FIREBASE_ANDROID_API_KEY=your-android-api-key
FIREBASE_ANDROID_APP_ID=your-android-app-id

# Firebase iOS Configuration
FIREBASE_IOS_API_KEY=your-ios-api-key
FIREBASE_IOS_APP_ID=your-ios-app-id
FIREBASE_IOS_BUNDLE_ID=com.yourcompany.yourapp

# Firebase Web Configuration (if applicable)
FIREBASE_WEB_API_KEY=your-web-api-key
FIREBASE_WEB_APP_ID=your-web-app-id
FIREBASE_WEB_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_WEB_MESSAGING_SENDER_ID=your-sender-id
FIREBASE_WEB_MEASUREMENT_ID=G-XXXXXXXXXX

# App Configuration
APP_NAME=Momo Timer
APP_VERSION=1.0.0
APP_BUILD_NUMBER=1

# Feature Flags (true/false)
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_VIBRATION=true
ENABLE_SOUND=true

# API Endpoints (if you have a backend)
# API_BASE_URL=https://api.yourservice.com
# API_TIMEOUT=30000

# Development Settings
DEBUG_MODE=false
LOG_LEVEL=info